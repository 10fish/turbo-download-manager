{
  "name": "Turbo Download Manager",
  "description": "a download manager with multi-threading support",
  "author": "Inbasic",
  "version": "0.1.8",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "notifications",
    "<all_urls>",
    "clipboardRead",
    "unlimitedStorage",
    {"fileSystem": ["write", "retainEntries", "directory"]}
  ],
  "app": {
    "background": {
      "scripts": [
        "lib/android/EventEmitter.js",
        "lib/android/md5.js",
        "lib/android/definitions.js",
        "lib/config.js",
        "lib/android/android.js",
         "lib/wget.js",
         "lib/icon.js",
         "lib/utils.js",
         "lib/mwget.js",
         "lib/common.js"
      ]
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  }
}
