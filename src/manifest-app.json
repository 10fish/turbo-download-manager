{
  "name": "Turbo Download Manager",
  "description": "a download manager with multi-threading support",
  "author": "Inbasic",
  "version": "0.2.3",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "notifications",
    "browser",
    "<all_urls>",
    "clipboardRead",
    "unlimitedStorage",
    "webview",
    {"fileSystem": ["write", "retainEntries", "directory"]}
  ],
  "app": {
    "background": {
      "scripts": [
        "lib/chrome/md5.js",
        "lib/utils.js",
        "lib/config.js",
        "lib/chrome/chrome.js",
        "lib/wget.js",
        "lib/icon.js",
        "lib/mwget.js"
      ]
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  }
}
